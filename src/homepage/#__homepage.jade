#__topNewsHome-main
    #__topNewsHome-centre
        #__topNewsHome-row1
          +M__heading('Breaking News', 'More News')
        #__topNewsHome-row2.u-cf
            #__topNewsHome-row2-col1.u-floatLeft
                +M__contentThumb--showcase
            #__topNewsHome-row2-col2.u-floatRight
                +M__largeSkyscraperAdvert--small

            #__topNewsHome-row2-col3.u-floatLeft
                .O__categoryList-main.u-cf
                    each val in [1,2,3,4,5,6]
                        .O__categoryList-item.u-floatLeft
                            +M__contentThumb--newsCrop

