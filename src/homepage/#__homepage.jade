#__featuresHome-main
    #__featuresHome-centre.u-cf
        #__featuresHome-col1.u-floatLeft
            #__featuresHome-col1-row1
                +M__heading('Popular News')
            #__featuresHome-col1-row2
            .O__categoryList-main.u-cf
                    each val in [1,2,3,4]
                        .O__categoryList-item.O__categoryList-item--fiveColumn.u-floatLeft
                            +M__contentThumb--newsCrop
            #__featuresHome-col1-row2
                +M__largeSkyscraperAdvert--superbanner
            #__featuresHome-col1-row3
                +M__heading('Popular News')
            #__featuresHome-col1-row4
                +M__contentThumb--showcase
        #__featuresHome-col2.u-floatRight
            +M__largeSkyscraperAdvert--islandAds
        #__featuresHome-col4.u-floatLeft
            #__featuresHome-col4-row1
                +M__heading('Feature', 'View more')
            #__featuresHome-col4-row2
                .O__categoryList-main.u-cf
                        each val in [1,2,3,4,5,6]
                            .O__categoryList-item.u-floatLeft
                                +M__contentThumb
        #__featuresHome-col5.u-floatRight
            +M__largeSkyscraperAdvert
        #__featuresHome-col6.u-floatLeft
            #__featuresHome-col4-row1
                +M__largeSkyscraperAdvert--superbanner
            #__featuresHome-col4-row2
                +M__heading('asdads')
            #__featuresHome-col4-row3
                +M__textPrompt--blue--noHead
            #__featuresHome-col4-row5
                +O__labeledLinksList--partnerFiles

