//-    LAYOUT                                                                   //
//-   ---------------                                                           //
//-    FEATURES HOME                                                            //
//-                                                                             //
//-                                                                             //
//-///////////////////////////////////////////////////////////////////////////////

#FeaturesHome-main
    #FeaturesHome-centre.u-cf
        #FeaturesHome-col1.u-floatLeft
            #FeaturesHome-col1-row1
                +Heading('Top Stories')
            #FeaturesHome-col1-row2.u-cf
                    each val in [1,2,3,4]
                        .u-col4.u-floatLeft
                            +Thumbnail--news
            #FeaturesHome-col1-row3
                +Advert--largeSuperbanner
            #FeaturesHome-col1-row4
                +Heading('Popular News')
            #FeaturesHome-col1-row5
                +Thumbnail--showcase
        #FeaturesHome-col2.u-floatRight
            +Advert--islandAdverts
        #FeaturesHome-col4.u-floatLeft
            #FeaturesHome-col4-row1
                +Heading('Feature', 'View more Features')
            #FeaturesHome-col4-row2
                each val in [1,2,3,4,5,6]
                    .u-col3.u-floatLeft
                        +Thumbnail



