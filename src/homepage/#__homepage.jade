//-///////////////////////////////////////////////////////////////////////////////

#NewsHome-main
    #NewsHome-centre
        #NewsHome-row1
          +Heading('Breaking News', 'More News')
        #NewsHome-row2.u-cf
            #NewsHome-row2-col1.u-floatLeft
                +Thumbnail--showcase
            #NewsHome-row2-col2.u-floatRight
                +Advert--smallSkyscraper

            #NewsHome-row2-col3.u-floatLeft.u-cf
                    each val in [1,2,3,4,5,6]
                        .u-col3.u-floatLeft
                            +Thumbnail--news

